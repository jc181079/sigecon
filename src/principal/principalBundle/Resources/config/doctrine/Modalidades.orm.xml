<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="principal\principalBundle\Entity\Modalidades" table="modalidades">
    <id name="idmodalidades" type="integer" column="idmodalidades">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nommodalidad" type="string" column="nommodalidad" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prefijo" type="string" column="prefijo" length="34" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correlativo" type="integer" column="correlativo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
