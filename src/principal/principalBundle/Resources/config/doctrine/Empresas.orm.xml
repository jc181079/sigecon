<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="principal\principalBundle\Entity\Empresas" table="empresas">
    <id name="idempresas" type="integer" column="idempresas">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreempre" type="string" column="nombreempre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rif" type="string" column="rif" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="objetosocial" type="text" column="objetosocial" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Lo que hace la empresa</option>
      </options>
    </field>
    <field name="representantelegal" type="string" column="representantelegal" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cedula" type="string" column="cedula" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="registromer" type="text" column="registromer" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nro" type="string" column="nro" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tomo" type="string" column="tomo" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechatomo" type="date" column="fechatomo" nullable="true"/>
  </entity>
</doctrine-mapping>
