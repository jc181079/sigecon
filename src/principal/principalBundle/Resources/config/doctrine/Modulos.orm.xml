<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="principal\principalBundle\Entity\Modulos" table="modulos">
    <indexes>
      <index name="fk_modulos_perfil1_idx" columns="idperfil"/>
    </indexes>
    <id name="idmodulos" type="integer" column="idmodulos">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nommodulo" type="string" column="nommodulo" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="observacionmod" type="string" column="observacionmod" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idperfil" target-entity="Perfil" fetch="LAZY">
      <join-columns>
        <join-column name="idperfil" referenced-column-name="idperfil"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
